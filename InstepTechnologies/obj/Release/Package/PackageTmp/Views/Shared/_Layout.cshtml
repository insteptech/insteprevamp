<body>

    <header id="header" class="container clearfix">

        <a href="~/Home/index" id="logo">
            <img src="~/img/logo.png" alt="SmartStart">
        </a>

        <nav id="main-nav">

            <ul>
                @if(ViewBag.PageName == "index")
                {
                <li class="current">
                    <a data-description="Lets start" href="~/Home/index">Home</a>
                </li>
                }else{
                     <li>
                         <a data-description="Lets start" href="~/Home/index">Home</a>
                </li>
                }

                @if (ViewBag.PageName == "aboutus")
                {
                    <li class="current">
                        <a data-description="Who we are"  href="~/Home/aboutus">About us</a>
                    </li>
                }
                else
                {
                      <li>
                          <a data-description="Who we are" href="~/Home/aboutus">About us</a>
                </li>
                }

                @if (ViewBag.PageName == "services")
                {
                    <li class="current">
                        <a data-description="What we offer" href="~/Home/services">Services</a>
                    </li>
                }
                else
                {
                    <li>
                        <a data-description="What we offer" href="~/Home/services">Services</a>
                    </li>
                }

                @if (ViewBag.PageName == "Training")
                {
                    <li class="current">
                        <a data-description="Learn with us" href="~/Home/training">Training</a>
                        <ul style="display: none;">
                            <li><a href="~/Home/trainingAngularjs">Angular JS</a></li>
                            <li><a href="~/Home/trainingMicrosoft">Microsft Technologies</a></li>
                            <li><a href="~/Home/trainingMean">MEAN Stack</a></li>
                        </ul>
                    </li>
                }
                else
                {
                    <li>
                        <a data-description="Learn with us" href="~/Home/training">Training</a>
                        <ul style="display: none;">
                            <li><a href="~/Home/trainingAngularjs">Angular JS</a></li>
                            <li><a href="~/Home/trainingMicrosoft">.net</a></li>
                            <li><a href="~/Home/trainingMean">MEAN Stack</a></li>
                        </ul>
                    </li>
                }
       
                @*@if (ViewBag.PageName == "blog")
                {
                    <li class="current">
                        <a href="~/Home/blog" data-description="What we think">Blog</a>
                    </li>
                }
                else
                {
                    <li>
                        <a href="~/Home/blog" data-description="enquire here">Blog</a>
                    </li>
                }*@

                @if (ViewBag.PageName == "contact")
                {
                    <li class="current">
                        <a data-description="Get in touch" href="~/Home/contact">Contact us</a>
                    </li>
                }
                else
                {
                    <li>
                        <a data-description="Get in touch" href="~/Home/contact">Contact us</a>
                    </li>
                }
            </ul>

        </nav><!-- end #main-nav -->

    </header><!-- end #header -->

    <section id="content" class="container clearfix" itemscope itemtype="http://schema.org/Organization">

        @RenderBody()
        @RenderSection("SPAViews", required: false)

    </section><!-- end #content -->

    <footer id="footer" class="clearfix">

        <div class="container">

            <div class="three-fourth">

                <nav id="footer-nav" class="clearfix">

                    <ul>
                        <li><a href="~/Home/index">Home</a></li>
                        <li><a href="~/Home/aboutus">About us</a></li>
                        <li><a href="~/Home/services">Services</a></li>
                        @*<li><a href="~/Home/projects">Projects</a></li>*@
                        <li><a href="~/Home/contact">Contact us</a></li>
                    </ul>

                </nav><!-- end #footer-nav -->

                <ul class="contact-info">
                    <li class="address">DSS 167, First Floor, Sector-25 panchkula, Haryana India</li>
                    <li class="phone">(IN)+91 86 99 953976</li>
                    <li class="phone">(UK)+44-(0)-7429567121</li>
                    <li class="email"><a href="mailto:office@insteptechnologies.com">office@insteptechnologies.com</a></li>
                    <li class="skype"> skype:insteptechnologies</li>
                </ul><!-- end .contact-info -->

            </div><!-- end .three-fourth -->

            <div class="one-fourth last">

                <span class="title">Stay connected</span>
                
                <ul class="social-links">
                    <li class="twitter"><a href="#">Twitter</a></li>
                    <li class="facebook"><a href="#">Facebook</a></li>
                    <li class="youtube"><a href="#">Linkedin</a></li>
                    <li class="skype"><a href="#">Skype</a></li>
                </ul><!-- end .social-links -->
                <!-- Place this tag where you want the share button to render. -->
                <div class="g-plus" data-action="share"></div>
            </div><!-- end .one-fourth.last -->

        </div><!-- end .container -->

    </footer><!-- end #footer -->

    <footer id="footer-bottom" class="clearfix">

        <div class="container">

            <ul>
                <li>InStep Technologies &copy; 2014</li>
                <li><a href="#">Legal Notice</a></li>
                <li><a href="#">Terms</a></li>
            </ul>

        </div><!-- end .container -->

    </footer><!-- end #footer-bottom -->
